// Barry
models/players/re_barry/torso_gen
{
    {
        map models/players/re_barry/torso_gen
        rgbGen lightingDiffuse
    }
    {
        map models/players/re_barry/torso_gen_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        alphaGen lightingSpecular
        detail
    }
}

