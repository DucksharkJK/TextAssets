{
	q3map_nolightmap
    {
        map models/weapons2/legosaber/mainhilt
        blendFunc GL_ONE GL_ZERO
        rgbGen lightingDiffuse
    }
    {
        map models/weapons2/legosaber/mainhilt_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
    {
        map models/weapons2/legosaber/mainhilt
