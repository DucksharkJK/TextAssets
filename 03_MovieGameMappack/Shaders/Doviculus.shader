models/players/Doviculus/Doviculus
{
	cull twosided
	q3map_nolightmap
    {
        map models/players/Doviculus/Doviculus
        blendFunc GL_ONE GL_ZERO
        rgbGen lightingDiffuse
    }
    {
        map  models/players/Doviculus/Doviculus_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        alphaGen lightingSpecular
    }
}

