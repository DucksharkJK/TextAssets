models/players/kotor_ha/body
{
    {
        map models/players/kotor_ha/body
        rgbGen lightingDiffuse
    }
    {
        map models/players/kotor_ha/cm_baremetal
        blendFunc GL_DST_COLOR GL_SRC_COLOR
        detail
        alphaGen lightingSpecular
        tcGen environment
    }
	{
        map models/players/kotor_ha/body
        rgbGen lightingDiffuse
        blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
    }
    {
        map models/players/kotor_ha/body_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

