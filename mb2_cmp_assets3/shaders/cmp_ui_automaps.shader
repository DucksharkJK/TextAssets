//####################
//##### Delta Prime
//####################
gfx/automap/deltaprime_1
{
	nopicmip
	nomipmaps
  {
	  clampmap gfx/automap/deltaprime_1
	  depthfunc equal
	  blendfunc blend
	  rgbGen identity
	  alphaGen const 0.85
  }
}

