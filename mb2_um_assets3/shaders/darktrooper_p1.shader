models/players/darktrooper_p1/eyes
{
    {
        map models/players/darktrooper_p1/eyes
        rgbGen lightingDiffuse
    }
    {
        map models/players/darktrooper_p1/eyesglow
        blendFunc GL_ONE GL_ONE
		glow
    }
}

models/players/darktrooper_p1/head
{
    {
        map models/players/darktrooper_p1/head
        rgbGen lightingDiffuse
    }
    {
        map models/players/darktrooper_p1/headref
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
    {
        map models/players/darktrooper_p1/headglow
        blendFunc GL_SRC_ALPHA GL_ONE
		glow
        alphaGen wave sin 0.75 1 0.25 10
    }
}

