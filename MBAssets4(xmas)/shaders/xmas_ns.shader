textures/xmas_ns/rouge
{
	qer_editorimage	textures/xmas_ns/rouge
	q3map_surfacelight	100
	q3map_lightRGB1 0.0859083 0.0328832
    {
        map $lightmap
    }
    {
        map textures/xmas_ns/rouge
        blendFunc GL_DST_COLOR GL_ZERO
    }
    {
        map textures/xmas_ns/rouge
        blendFunc GL_ONE GL_ONE
        glow
    }
}

