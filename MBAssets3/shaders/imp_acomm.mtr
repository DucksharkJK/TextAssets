models/players/imp_acomm/armor
{
    {		
        map models/players/imp_acomm/armor_A
        normalMap models/players/imp_acomm/armor_N
		rmoMap models/players/imp_acomm/armor_RMO
		rgbGen lightingDiffuse
    }    
    {
        map models/players/imp_acomm/armor_e
        blendFunc GL_ONE GL_ONE
        glow
        detail
        rgbGen identity
    }
}

