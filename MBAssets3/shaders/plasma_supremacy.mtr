//Mirror

textures/plasma_sup/mirror_floor
{
	qer_editorimage	textures/plasma_sup/mirror_floor
	portal
	q3map_nolightmap
    {
        map textures/plasma_sup/mirror_floor
        blendFunc GL_ONE GL_ONE_MINUS_SRC_ALPHA
        depthWrite
    }
    {
        map textures/plasma_sup/mirror_floor
        blendFunc GL_DST_COLOR GL_SRC_COLOR
    }
    {
        map textures/plasma_tfa/skb_env
        blendFunc GL_ONE GL_ONE
        rgbGen const ( 0.200000 0.200000 0.200000 )
        tcGen environment
		detail
    }
}

