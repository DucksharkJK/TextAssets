textures/amace_dantooine/LightTrim01
{
	qer_editorimage	textures/amace_dantooine/LightTrim01
	q3map_lightimage	textures/amace_dantooine/LightTrim01_E
	q3map_surfacelight 1600
	q3map_backSplash 0.5 8
    {
        map $lightmap
    }
	{
		map textures/amace_dantooine/LightTrim01_A
		normalMap textures/amace_dantooine/LightTrim01_N
		rmoMap textures/amace_dantooine/LightTrim01_RMO
		blendFunc GL_DST_COLOR GL_ZERO
	}
    {
        map textures/amace_dantooine/LightTrim01_E
        blendFunc GL_ONE GL_ONE
        glow
        rgbGen identity
    }
}

