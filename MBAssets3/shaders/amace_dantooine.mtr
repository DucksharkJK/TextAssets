textures/amace_dantooine/gardenglass
{
	qer_editorimage	textures/amace_leviathan/glass1
	qer_trans	0.5
	surfaceparm	trans
	q3map_material	Glass
    {
        map $whiteimage
        alphaGen const 0.0
        alphaFunc GE128
        depthWrite
    }
    {
        map $whiteimage
        specularMap $whiteimage
        normalMap textures/common/glass_n
        normalScale 0.015 0.015
        alphaGen const 0.06
        rgbGen vertex
        blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
    }
}

