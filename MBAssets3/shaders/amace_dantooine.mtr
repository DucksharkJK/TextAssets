textures/amace_dantooine/Grass01_Rock01_Blend
{
	qer_editorimage	textures/amace_dantooine/Rock01
	q3map_shadeangle	120
	q3map_nonplanar
    {
// Primary
        map textures/amace_dantooine/Rock01_A
		normalMap textures/amace_dantooine/Rock01_N
		rmoMap textures/amace_dantooine/Rock01_RMO
        rgbGen identity
    }
    {
// Secondary
        map textures/amace_dantooine/Grass01_A
		normalHeightMap textures/amace_dantooine/Grass01_NH
		rmoMap textures/amace_dantooine/Grass01_RMO
        blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
        rgbGen identity
        alphaGen vertex
		tcMod scale	3 3
    }
    {
        map $lightmap
        blendFunc GL_DST_COLOR GL_ZERO
        rgbGen identity
	}
}

