models/players/siobibble/torso
{
	cull	twosided
    {
        map models/players/siobibble/torso
        rgbGen lightingDiffuse
    }
    {
        map models/players/siobibble/torso_s
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

