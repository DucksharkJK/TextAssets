models/players/jocastanu/hair
{
	q3map_nolightmap
    {
        map models/players/jocastanu/hair
        rgbGen lightingDiffuse
    }
    {
        map models/players/jocastanu/hair_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

models/players/jocastanu/head
{
	q3map_nolightmap
    {
        map models/players/jocastanu/head
        rgbGen lightingDiffuse
    }
    {
        map models/players/jocastanu/head_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}