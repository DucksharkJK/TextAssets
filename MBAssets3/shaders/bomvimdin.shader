models/players/bomvimdin/bomhead
{
	q3map_nolightmap
    {
        map models/players/bomvimdin/bomhead
        rgbGen lightingDiffuse
    }
    {
        map models/players/bomvimdin/bomhead_s
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

