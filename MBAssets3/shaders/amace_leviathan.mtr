//////////// Light and Glow Shaders ////////////
textures/amace_leviathan/floor_sign
{
	q3map_lightimage	textures/amace_leviathan/floor_sign_glow
	qer_editorimage	textures/amace_leviathan/floor_sign
	q3map_surfacelight	1700
	q3map_lightsubdivide	512
    {
        map $lightmap
    }
    {
        map textures/amace_leviathan/floor_sign_A
		normalMap textures/amace_leviathan/floor_sign_N
		rmoMap textures/amace_leviathan/floor_sign_RMO
        blendFunc GL_DST_COLOR GL_ZERO
    }
    {
        map textures/amace_leviathan/floor_sign_E
        blendFunc GL_ONE GL_ONE
        glow
        rgbGen identity
    }
}

